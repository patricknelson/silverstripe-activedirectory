{
    "name": "silverstripe/saml",
    "description": "Adds support to SilverStripe for SAML based SSO authentication",
    "type": "silverstripe-module",
    "keywords": ["silverstripe", "sso", "saml"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sean Harvey",
            "email": "sean@silverstripe.com"
        },
        {
            "name": "Stig Lindqvist",
            "email": "stig@silverstripe.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "silverstripe/framework": "^4",
        "silverstripe/cms": "^4",
        "silverstripe/queuedjobs": "^4.0@dev",
        "zendframework/zend-ldap": "^2.5.1",
        "zendframework/zend-authentication": "^2.5.1",
        "zendframework/zend-session": "^2.5.1",
        "onelogin/php-saml": "^2.10.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ActiveDirectory\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
